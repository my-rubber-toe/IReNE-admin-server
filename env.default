FLASK_APP=app
FLASK_ENV=development
FLASK_DEBUG=1

# System port to use
PORT=<port>

# Database
DB_HOST=<yout-database-connection-string>

# Emails
ENABLE_EMAIL=1
EMAIL_SENDER=<sender-email>
EMAIL_PASSWD=<sender-email-password>

# Flask secret
FLASK_SECRET_KEY=<random-string>
FLASK_SALT=<another-random-string>